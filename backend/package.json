{
  "name": "dap-backend",
  "version": "2.3.3",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "cp .env.development .env 2>/dev/null || true && ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:devtools": "ts-node-dev --respawn --transpile-only src/devtools-service.ts",
    "build": "tsc -p tsconfig.json",
    "build:dev": "cp .env.development .env && npm run build",
    "build:prod": "cp .env.production .env && npm run build",
    "start": "node dist/server.js",
    "start:dev": "cp .env.development .env && node dist/server.js",
    "start:prod": "cp .env.production .env && node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "seed": "ts-node src/seed.ts",
    "seed:sample": "ts-node scripts/add-laptop-sample.ts",
    "seed:dev": "ts-node src/seed-dev.ts",
    "fix-auth": "ts-node scripts/fix_user_auth.ts",
    "reset": "ts-node scripts/reset-dev-db.ts",
    "test": "jest --runInBand --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --runInBand",
    "test:ci": "jest --coverage --runInBand --ci",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@graphql-tools/schema": "^10.0.0",
    "@prisma/client": "^6.14.0",
    "@sentry/node": "^10.28.0",
    "@sentry/profiling-node": "^10.28.0",
    "@types/node-cron": "^3.0.11",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dataloader": "^2.2.3",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "graphql": "^16.9.0",
    "graphql-relay": "^0.10.1",
    "graphql-tag": "^2.12.6",
    "graphql-upload": "^16.0.2",
    "graphql-ws": "^5.16.2",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "papaparse": "^5.4.1",
    "pino": "^9.0.0",
    "pino-pretty": "^11.3.0",
    "ws": "^8.17.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@faker-js/faker": "^9.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/graphql-upload": "^17.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.0.0",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.5.10",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prisma": "^6.14.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.15.0"
  }
}