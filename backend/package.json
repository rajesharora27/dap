    "@prisma/client": "^6.14.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "graphql": "^16.9.0",
    "graphql-relay": "^0.10.1",
    "graphql-tag": "^2.12.6",
    "graphql-upload": "^16.0.2",
    "graphql-ws": "^5.16.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "papaparse": "^5.4.1",
    "pino": "^9.0.0",
    "pino-pretty": "^11.3.0",
    "ws": "^8.17.0"
