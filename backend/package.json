    "@types/graphql-upload": "^17.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.10",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.5.10",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prisma": "^6.14.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.15.0"
